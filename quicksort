int main()
{
    //quicksort algo;
    printf("enter the number of elements in the array\n");
    int n;  
    scanf("%d", &n);
    
    int arr[n]; //the number of elements in the array is generated
    
    srand(time(0));
    for(int i = 0; i < n; i++)
    {
        arr[i] = rand()%100;
    }
    clock_t begin = clock();
    
    quicksort(arr, arr[0], arr[n-1]); // calling the quicksort function
    
    clock_t end = clock();
    
    float time_spent = (float)(end - begin)/CLOCKS_PER_SEC;
    printf(" time == %f" , time_spent);
}

